programa {
  funcao inicio() {
    real n1, n2, media
    escreva("Escreva qual sua nota no primeiro bimestre: ")
    leia(n1)
    escreva("Escreva qual a sua nota no segundo bimestre: ")
    leia(n2)
    media = n1/2+n2/2
    escreva("Sua nota sera: ", media)
    
  }
}
